<Window x:Class="Calculatrice.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Calculatrice"
        xmlns:diag="clr-namespace:System.Diagnostics;assembly=WindowsBase"
        xmlns:controls="clr-namespace:Calculatrice.CustomControls"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        mc:Ignorable="d"
        Title="MainWindow" Height="855" Width="375" WindowStyle="None" AllowsTransparency="True" Background="Transparent"        
        WindowStartupLocation="Manual">
  
    <Window.Resources>
        <Style TargetType="local:MainWindow">
            <Setter Property="Opacity" Value="1"/>
        </Style>
        <!--Accession config.xml-->
        <XmlDataProvider Source="Configuration\config.xml" XPath="/ConfigurationUI/Calculatrice" x:Key="configurationUI" x:Name="configurationUI"/>
        <SolidColorBrush x:Key="BackgroundScreen" Color="{Binding Source={StaticResource configurationUI}, XPath=BackgroundScreen/@value}"/>
        <SolidColorBrush x:Key="BackgroundFunc" Color="{Binding Source={StaticResource configurationUI}, XPath=BackgroundFunc/@value}"/>
        <SolidColorBrush x:Key="BackgroundCornerScreen" Color="{Binding Source={StaticResource configurationUI}, XPath=BackgroundCornerScreen/@value}"/>
        <SolidColorBrush x:Key="LeftLabelBackground" Color="{Binding Source={StaticResource configurationUI}, XPath=LeftLabelBackground/@value}"/>
        <SolidColorBrush x:Key="RightLabelBackground" Color="{Binding Source={StaticResource configurationUI}, XPath=RightLabelBackground/@value}"/>
        <SolidColorBrush x:Key="LeftLabelForeground" Color="{Binding Source={StaticResource configurationUI}, XPath=LeftLabelForeground/@value}"/>
        <SolidColorBrush x:Key="RightLabelForeground" Color="{Binding Source={StaticResource configurationUI}, XPath=RightLabelForeground/@value}"/>
        <SolidColorBrush x:Key="FuncControl1Background" Color="{Binding Source={StaticResource configurationUI}, XPath=FuncControl1Background/@value}"/>
        <SolidColorBrush x:Key="FuncControl1Foreground" Color="{Binding Source={StaticResource configurationUI}, XPath=FuncControl1Foreground/@value}"/>
        <SolidColorBrush x:Key="FuncControl2Background" Color="{Binding Source={StaticResource configurationUI}, XPath=FuncControl2Background/@value}"/>
        <SolidColorBrush x:Key="FuncControl2Foreground" Color="{Binding Source={StaticResource configurationUI}, XPath=FuncControl2Foreground/@value}"/>
        <SolidColorBrush x:Key="FuncControl3Background" Color="{Binding Source={StaticResource configurationUI}, XPath=FuncControl3Background/@value}"/>
        <SolidColorBrush x:Key="FuncControl3Foreground" Color="{Binding Source={StaticResource configurationUI}, XPath=FuncControl3Foreground/@value}"/>
        <SolidColorBrush x:Key="FuncControl4Background" Color="{Binding Source={StaticResource configurationUI}, XPath=FuncControl4Background/@value}"/>
        <SolidColorBrush x:Key="FuncControl4Foreground" Color="{Binding Source={StaticResource configurationUI}, XPath=FuncControl4Foreground/@value}"/>
        <SolidColorBrush x:Key="EcranCalculGrapheBackground" Color="{Binding Source={StaticResource configurationUI}, XPath=EcranCalculGrapheBackground/@value}"/>
        <SolidColorBrush x:Key="DirectionControlTemplateShapeFill" Color="{Binding Source={StaticResource configurationUI}, XPath=DirectionControlTemplateShapeFill/@value}"/>
        <SolidColorBrush x:Key="DirectionControlTemplateShapeStroke" Color="{Binding Source={StaticResource configurationUI}, XPath=DirectionControlTemplateShapeStroke/@value}"/>
        <SolidColorBrush x:Key="DirectionControlTemplateTriangleFill" Color="{Binding Source={StaticResource configurationUI}, XPath=DirectionControlTemplateTriangleFill/@value}"/>
        <SolidColorBrush x:Key="DirectionControlTemplateTriangleStroke" Color="{Binding Source={StaticResource configurationUI}, XPath=DirectionControlTemplateTriangleStroke/@value}"/>
        <SolidColorBrush x:Key="DirectionControlTemplateTriangleStrokeThickness" Color="{Binding Source={StaticResource configurationUI}, XPath=DirectionControlTemplateTriangleStrokeThickness/@value}"/>
        <Style TargetType="controls:FunctControl">
            <Setter Property="FuncBackground" Value="{StaticResource FuncControl2Background}"/>
            <Setter Property="FuncForeground" Value="{StaticResource FuncControl2Foreground}"/>
            <Setter Property="LeftLabelBackground" Value="{StaticResource LeftLabelBackground}"/>
            <Setter Property="LeftLabelForeground" Value="{StaticResource LeftLabelForeground}"/>
            <Setter Property="RightLabelBackground" Value="{StaticResource RightLabelBackground}"/>
            <Setter Property="RightLabelForeground" Value="{StaticResource RightLabelForeground}"/>
        </Style>
    </Window.Resources>
    
    <DockPanel UseLayoutRounding="True">
        <Border CornerRadius="55 55 0 0" BorderThickness="5 5 5 0" BorderBrush="Black" Background="{StaticResource BackgroundScreen}" DockPanel.Dock="Top">
