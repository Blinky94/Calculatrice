                                        <Setter Property="FuncTextSize" Value="{Binding Source={StaticResource configurationUI}, XPath=FuncTextSize/@value}"/>
                                        <Setter Property="FuncBackground" Value="{Binding Source={StaticResource RightLabelForeground}}"/>
                                        <Setter Property="FuncForeground" Value="{Binding Source={StaticResource FuncControl1Background}}"/>
                                        <Setter Property="FuncLabelLeftMargin" Value="-8"/>
                                        <Setter Property="FuncLabelRightMargin" Value="-12 0 0 0"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </controls:FunctControl.Style>
                    </controls:FunctControl>
                    <controls:FunctControl x:Name="calculs" FuncHeight="22" FuncButtonMargin="0 -20 0 0" FuncTextMargin="">
