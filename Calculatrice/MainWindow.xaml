                <StackPanel Orientation="Vertical" HorizontalAlignment="Center">
                    <!--Ecran de calcul/Graphe-->
                    <StackPanel Orientation="Horizontal" Margin="0 -40 0 0" HorizontalAlignment="Center">
                        <TextBlock Text="TI-83" TextAlignment="Center" FontSize="18" FontWeight="Bold" Foreground="Azure" Margin="0 2.5 0 0"/>
                        <TextBlock Text=" Premium" TextAlignment="Center" FontSize="18" FontWeight="Normal" Foreground="LightGray" Margin="0 2.5 0 0"/>
                        <TextBlock Text=" CE" TextAlignment="Center" FontSize="20" FontWeight="Thin" Foreground="Gray">
                            <TextBlock.Effect>
                                <DropShadowEffect/>
                            </TextBlock.Effect>
                        </TextBlock>
                    </StackPanel>
                    <Border Margin="15 20 15 15" Background="{StaticResource EcranCalculGrapheBackground}">
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                            <Canvas Name="Canvassss" Width="300" Height="195">
                                <RichTextBox Name="EcranPrincipal" Width="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type Canvas}}, Path=Width}" Height="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type Canvas}}, Path=Height}">
                                    <FlowDocument>
                                        <Paragraph>Ceci est un test</Paragraph>
                                    </FlowDocument>
                                </RichTextBox>
                            </Canvas>
                        </StackPanel>
                    </Border>
                </StackPanel>
            </Border>
        </Border>

