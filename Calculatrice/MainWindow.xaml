<Window x:Class="Calculatrice.MainWindow" 
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:local="clr-namespace:Calculatrice" 
        xmlns:controls="clr-namespace:Calculatrice.CustomControls" 
        mc:Ignorable="d" Title="MainWindow" Height="855" Width="375" WindowStyle="None" AllowsTransparency="True" Background="Transparent" WindowStartupLocation="Manual">

    <Window.Resources>
        <Style TargetType="local:MainWindow">
            <Setter Property="Opacity" Value="1" />
        </Style>
        <!--Accession config.xml-->
        <XmlDataProvider Source="Configuration\config.xml" XPath="/ConfigurationUI/Calculatrice" x:Key="configurationUI" x:Name="configurationUI" />
        <SolidColorBrush x:Key="BackgroundScreen" Color="{Binding Source={StaticResource configurationUI}, XPath=BackgroundScreen/@value}" />
        <SolidColorBrush x:Key="BackgroundFunc" Color="{Binding Source={StaticResource configurationUI}, XPath=BackgroundFunc/@value}" />
        <SolidColorBrush x:Key="BackgroundCornerScreen" Color="{Binding Source={StaticResource configurationUI}, XPath=BackgroundCornerScreen/@value}" />
        <SolidColorBrush x:Key="LeftLabelBackground" Color="{Binding Source={StaticResource configurationUI}, XPath=LeftLabelBackground/@value}" />
        <SolidColorBrush x:Key="RightLabelBackground" Color="{Binding Source={StaticResource configurationUI}, XPath=RightLabelBackground/@value}" />
        <SolidColorBrush x:Key="LeftLabelForeground" Color="{Binding Source={StaticResource configurationUI}, XPath=LeftLabelForeground/@value}" />
        <SolidColorBrush x:Key="RightLabelForeground" Color="{Binding Source={StaticResource configurationUI}, XPath=RightLabelForeground/@value}" />
        <SolidColorBrush x:Key="FuncControl1Background" Color="{Binding Source={StaticResource configurationUI}, XPath=FuncControl1Background/@value}" />
        <SolidColorBrush x:Key="FuncControl1Foreground" Color="{Binding Source={StaticResource configurationUI}, XPath=FuncControl1Foreground/@value}" />
        <SolidColorBrush x:Key="FuncControl2Background" Color="{Binding Source={StaticResource configurationUI}, XPath=FuncControl2Background/@value}" />
        <SolidColorBrush x:Key="FuncControl2Foreground" Color="{Binding Source={StaticResource configurationUI}, XPath=FuncControl2Foreground/@value}" />
        <SolidColorBrush x:Key="FuncControl3Background" Color="{Binding Source={StaticResource configurationUI}, XPath=FuncControl3Background/@value}" />
        <SolidColorBrush x:Key="FuncControl3Foreground" Color="{Binding Source={StaticResource configurationUI}, XPath=FuncControl3Foreground/@value}" />
        <SolidColorBrush x:Key="FuncControl4Background" Color="{Binding Source={StaticResource configurationUI}, XPath=FuncControl4Background/@value}" />
        <SolidColorBrush x:Key="FuncControl4Foreground" Color="{Binding Source={StaticResource configurationUI}, XPath=FuncControl4Foreground/@value}" />
        <SolidColorBrush x:Key="EcranCalculGrapheBackground" Color="{Binding Source={StaticResource configurationUI}, XPath=EcranCalculGrapheBackground/@value}" />
        <SolidColorBrush x:Key="DirectionControlTemplateShapeFill" Color="{Binding Source={StaticResource configurationUI}, XPath=DirectionControlTemplateShapeFill/@value}" />
        <SolidColorBrush x:Key="DirectionControlTemplateShapeStroke" Color="{Binding Source={StaticResource configurationUI}, XPath=DirectionControlTemplateShapeStroke/@value}" />
        <SolidColorBrush x:Key="DirectionControlTemplateTriangleFill" Color="{Binding Source={StaticResource configurationUI}, XPath=DirectionControlTemplateTriangleFill/@value}" />
        <SolidColorBrush x:Key="DirectionControlTemplateTriangleStroke" Color="{Binding Source={StaticResource configurationUI}, XPath=DirectionControlTemplateTriangleStroke/@value}" />
        <SolidColorBrush x:Key="DirectionControlTemplateTriangleStrokeThickness" Color="{Binding Source={StaticResource configurationUI}, XPath=DirectionControlTemplateTriangleStrokeThickness/@value}" />
        <Style TargetType="controls:FunctControl">
            <Setter Property="FuncBackground" Value="{StaticResource FuncControl2Background}" />
            <Setter Property="FuncForeground" Value="{StaticResource FuncControl2Foreground}" />
            <Setter Property="LeftLabelBackground" Value="{StaticResource LeftLabelBackground}" />
            <Setter Property="LeftLabelForeground" Value="{StaticResource LeftLabelForeground}" />
            <Setter Property="RightLabelBackground" Value="{StaticResource RightLabelBackground}" />
            <Setter Property="RightLabelForeground" Value="{StaticResource RightLabelForeground}" />
        </Style>
    </Window.Resources>

    <DockPanel UseLayoutRounding="True">
        <Border CornerRadius="55 55 0 0" BorderThickness="5 5 5 0" BorderBrush="Black" Background="{StaticResource BackgroundScreen}" DockPanel.Dock="Top">
            <Border CornerRadius="25" Background="{StaticResource BackgroundCornerScreen}" DockPanel.Dock="Bottom" Margin="25 42 25 10" Padding="10 0 10 5">
                <StackPanel Orientation="Vertical" HorizontalAlignment="Center">
                 
                    <!--Ecran de calcul/Graphe-->                  
                    <StackPanel Orientation="Horizontal" Margin="0 -40 0 0" HorizontalAlignment="Center">
                        <TextBlock Text="TI-83" TextAlignment="Center" FontSize="18" FontWeight="Bold" Foreground="Azure" Margin="0 2.5 0 0" />
                        <TextBlock Text=" Premium" TextAlignment="Center" FontSize="18" FontWeight="Normal" Foreground="LightGray" Margin="0 2.5 0 0" />
                        <TextBlock Text=" CE" TextAlignment="Center" FontSize="20" FontWeight="Thin" Foreground="Gray">
                            <TextBlock.Effect>
                                <DropShadowEffect />
                            </TextBlock.Effect>
                        </TextBlock>
                    </StackPanel>
                    <Border Margin="10 20 10 15" Background="{StaticResource EcranCalculGrapheBackground}">
                        <StackPanel Orientation="Horizontal">
                            <Canvas Name="Display" Width="260" Height="210" Block.LineStackingStrategy="MaxHeight">
                                <RichTextBox Name="RichTextBoxDisplay" Width="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type Canvas}}, Path=Width}" Height="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type Canvas}}, Path=Height}" FontFamily="{StaticResource Digital}" FontSize="24" CaretBrush="Black" IsReadOnly="False" IsReadOnlyCaretVisible="False">
                                    <FlowDocument x:Name="NumericDisplay" IsEnabled="False" />
                                </RichTextBox>
                                <Border x:Name="Caret" Visibility="Visible" Canvas.Left="5" Canvas.Top="2" Width="12" Height="21" Background="Green" Opacity="0.5">
                                    <Border.Triggers>
                                        <EventTrigger RoutedEvent="Border.Loaded">
                                            <BeginStoryboard>
                                                <Storyboard x:Name="CaretStoryBoard" RepeatBehavior="Forever">
                                                    <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="Background.Color" Duration="0:0:0:1" FillBehavior="HoldEnd">
                                                        <ColorAnimationUsingKeyFrames.KeyFrames>
                                                            <DiscreteColorKeyFrame KeyTime="0:0:0.750" Value="Transparent" />
                                                            <DiscreteColorKeyFrame KeyTime="0:0:0.000" Value="Green" />
                                                        </ColorAnimationUsingKeyFrames.KeyFrames>
                                                    </ColorAnimationUsingKeyFrames>
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </EventTrigger>
                                    </Border.Triggers>
                                </Border>
                            </Canvas>
                        </StackPanel>
                    </Border>
                </StackPanel>
            </Border>
        </Border>

        <Border CornerRadius="0 0 55 55" BorderThickness="5 0 5 5" BorderBrush="Black" Background="{StaticResource BackgroundFunc}" DockPanel.Dock="Bottom">
            <StackPanel Orientation="Vertical" HorizontalAlignment="Center">
                <StackPanel.Resources>
                    <Style x:Key="FuncGlobalStyle2" TargetType="{x:Type controls:FunctControl}" BasedOn="{StaticResource {x:Type controls:FunctControl}}">
                        <Setter Property="LeftLabelSize" Value="{Binding Source={StaticResource configurationUI}, XPath=LeftLabelSize/@value}" />
                        <Setter Property="RightLabelSize" Value="{Binding Source={StaticResource configurationUI}, XPath=RightLabelSize/@value}" />
                        <Setter Property="FuncTextSize" Value="{Binding Source={StaticResource configurationUI}, XPath=FuncTextSize/@value}" />
                        <Setter Property="FuncBackground" Value="{StaticResource FuncControl2Background}" />
                        <Setter Property="FuncForeground" Value="{StaticResource FuncControl2Foreground}" />
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding ElementName=Snde, Path=FuncButtonIsChecked}" Value="True">
                                <Setter Property="FuncTextSize" Value="15" />
                            </DataTrigger>
                            <DataTrigger Binding="{Binding ElementName=Alpha, Path=FuncButtonIsChecked}" Value="True">
                                <Setter Property="FuncTextSize" Value="15" />
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                    <Style x:Key="FuncGlobalStyle3" TargetType="{x:Type controls:FunctControl}" BasedOn="{StaticResource {x:Type controls:FunctControl}}">
                        <Setter Property="LeftLabelSize" Value="{Binding Source={StaticResource configurationUI}, XPath=LeftLabelSize/@value}" />
                        <Setter Property="RightLabelSize" Value="{Binding Source={StaticResource configurationUI}, XPath=RightLabelSize/@value}" />
                        <Setter Property="FuncBackground" Value="{StaticResource FuncControl3Background}" />
                        <Setter Property="FuncForeground" Value="{StaticResource FuncControl3Foreground}" />
                        <Setter Property="FuncTextSize" Value="28" />
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding ElementName=Snde, Path=FuncButtonIsChecked}" Value="True">
                                <Setter Property="FuncTextSize" Value="15" />
                            </DataTrigger>
                            <DataTrigger Binding="{Binding ElementName=Alpha, Path=FuncButtonIsChecked}" Value="True">
                                <Setter Property="FuncTextSize" Value="15" />
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </StackPanel.Resources>

                <!--Touches de configuration de graphe/tracé-->
                <StackPanel Orientation="Horizontal" Margin="20 15 20 5">
                    <controls:FunctControl x:Name="GraphStats" FuncHeight="22" FuncButtonMargin="0 -20 0 0" FuncTextMargin="" FuncButtonCornerRadius="12 5 5 12" ToggleButtonClick="GraphStats_ToggleButtonClick">
                        <controls:FunctControl.Style>
                            <Style TargetType="controls:FunctControl" BasedOn="{StaticResource FuncGlobalStyle2}">
                                <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=graphStats/LeftLabel/@value}" />
                                <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=graphStats/RightLabel/@value}" />
                                <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=graphStats/FuncText/@value}" />
                                <Setter Property="FuncLabelLeftMargin" Value="-15" />
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding ElementName=Snde, Path=FuncButtonIsChecked}" Value="True">
                                        <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=graphStats/LeftLabel/@value}" />
                                        <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=graphStats/FuncText/@value}" />
                                        <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=graphStats/RightLabel/@value}" />
                                        <Setter Property="FuncTextSize" Value="9" />
                                        <Setter Property="FuncLabelLeftMargin" Value="0 0 0 0" />
                                        <Setter Property="FuncLabelRightMargin" Value="0 0 0 0" />
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding ElementName=Alpha, Path=FuncButtonIsChecked}" Value="True">
                                        <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=graphStats/RightLabel/@value}" />
                                        <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=graphStats/LeftLabel/@value}" />
                                        <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=graphStats/FuncText/@value}" />
                                        <Setter Property="FuncLabelLeftMargin" Value="-15" />
                                        <Setter Property="FuncLabelRightMargin" Value="-5 0 0 0" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </controls:FunctControl.Style>
                    </controls:FunctControl>

                    <controls:FunctControl x:Name="DefTable" FuncHeight="22" FuncButtonMargin="0 -20 0 0" FuncTextMargin="" ToggleButtonClick="DefTable_ToggleButtonClick">
                        <controls:FunctControl.Style>
                            <Style TargetType="controls:FunctControl" BasedOn="{StaticResource FuncGlobalStyle2}">
                                <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=defTable/LeftLabel/@value}" />
                                <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=defTable/RightLabel/@value}" />
                                <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=defTable/FuncText/@value}" />
                                <Setter Property="FuncTextSize" Value="10" />
                                <Setter Property="FuncLabelLeftMargin" Value="-10" />
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding ElementName=Snde, Path=FuncButtonIsChecked}" Value="True">
                                        <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=defTable/LeftLabel/@value}" />
                                        <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=defTable/FuncText/@value}" />
                                        <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=defTable/RightLabel/@value}" />
                                        <Setter Property="FuncTextSize" Value="9" />
                                        <Setter Property="FuncLabelLeftMargin" Value="-10" />
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding ElementName=Alpha, Path=FuncButtonIsChecked}" Value="True">
                                        <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=defTable/RightLabel/@value}" />
                                        <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=defTable/LeftLabel/@value}" />
                                        <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=defTable/FuncText/@value}" />
                                        <Setter Property="FuncLabelLeftMargin" Value="-14" />
                                        <Setter Property="FuncLabelRightMargin" Value="-17 0 0 0" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </controls:FunctControl.Style>
                    </controls:FunctControl>
                    <controls:FunctControl x:Name="Format" FuncHeight="22" FuncButtonMargin="0 -20 0 0" FuncTextMargin="" ToggleButtonClick="Format_ToggleButtonClick">
                        <controls:FunctControl.Style>
                            <Style TargetType="controls:FunctControl" BasedOn="{StaticResource FuncGlobalStyle2}">
                                <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=format/LeftLabel/@value}" />
                                <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=format/RightLabel/@value}" />
                                <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=format/FuncText/@value}" />
                                <Setter Property="FuncLabelLeftMargin" Value="-10" />
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding ElementName=Snde, Path=FuncButtonIsChecked}" Value="True">
                                        <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=format/LeftLabel/@value}" />
                                        <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=format/FuncText/@value}" />
                                        <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=format/RightLabel/@value}" />
                                        <Setter Property="FuncTextSize" Value="9" />
                                        <Setter Property="FuncLabelLeftMargin" Value="-5" />
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding ElementName=Alpha, Path=FuncButtonIsChecked}" Value="True">
                                        <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=format/RightLabel/@value}" />
                                        <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=format/LeftLabel/@value}" />
                                        <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=format/FuncText/@value}" />
                                        <Setter Property="FuncLabelLeftMargin" Value="-8" />
                                        <Setter Property="FuncLabelRightMargin" Value="-12 0 0 0" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </controls:FunctControl.Style>
                    </controls:FunctControl>
                    <controls:FunctControl x:Name="Calculs" FuncHeight="22" FuncButtonMargin="0 -20 0 0" FuncTextMargin="" ToggleButtonClick="Calculs_ToggleButtonClick">
                        <controls:FunctControl.Style>
                            <Style TargetType="controls:FunctControl" BasedOn="{StaticResource FuncGlobalStyle2}">
                                <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=calculs/LeftLabel/@value}" />
                                <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=calculs/RightLabel/@value}" />
                                <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=calculs/FuncText/@value}" />
                                <Setter Property="FuncLabelLeftMargin" Value="-10" />
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding ElementName=Snde, Path=FuncButtonIsChecked}" Value="True">
                                        <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=calculs/LeftLabel/@value}" />
                                        <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=calculs/FuncText/@value}" />
                                        <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=calculs/RightLabel/@value}" />
                                        <Setter Property="FuncTextSize" Value="10" />
                                        <Setter Property="FuncLabelLeftMargin" Value="-5" />
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding ElementName=Alpha, Path=FuncButtonIsChecked}" Value="True">
                                        <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=calculs/RightLabel/@value}" />
                                        <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=calculs/LeftLabel/@value}" />
                                        <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=calculs/FuncText/@value}" />
                                        <Setter Property="FuncLabelLeftMargin" Value="-10" />
                                        <Setter Property="FuncLabelRightMargin" Value="-10 0 0 0" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </controls:FunctControl.Style>
                    </controls:FunctControl>
                    <controls:FunctControl x:Name="Table" FuncHeight="22" FuncButtonMargin="0 -20 0 0" FuncTextMargin="" FuncButtonCornerRadius="5 12 12 5" ToggleButtonClick="Table_ToggleButtonClick">
                        <controls:FunctControl.Style>
                            <Style TargetType="controls:FunctControl" BasedOn="{StaticResource FuncGlobalStyle2}">
                                <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=table/LeftLabel/@value}" />
                                <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=table/RightLabel/@value}" />
                                <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=table/FuncText/@value}" />
                                <Setter Property="FuncTextSize" Value="10" />
                                <Setter Property="FuncLabelLeftMargin" Value="-10" />
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding ElementName=Snde, Path=FuncButtonIsChecked}" Value="True">
                                        <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=table/LeftLabel/@value}" />
                                        <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=table/FuncText/@value}" />
                                        <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=table/RightLabel/@value}" />
                                        <Setter Property="FuncTextSize" Value="10" />
                                        <Setter Property="FuncLabelLeftMargin" Value="-5" />
                                    </DataTrigger>
                                    <DataTrigger Binding="{Binding ElementName=Alpha, Path=FuncButtonIsChecked}" Value="True">
                                        <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=table/RightLabel/@value}" />
                                        <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=table/LeftLabel/@value}" />
                                        <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=table/FuncText/@value}" />
                                        <Setter Property="FuncLabelLeftMargin" Value="-10" />
                                        <Setter Property="FuncLabelRightMargin" Value="-18 0 0 0" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </controls:FunctControl.Style>
                    </controls:FunctControl>
                </StackPanel>

                <!--Touches d'édition-->
                <StackPanel Orientation="Horizontal" Margin="20 0 20 5">
                    <StackPanel Orientation="Vertical">
                        <StackPanel Orientation="Horizontal">
                            <controls:FunctControl x:Name="Snde" ToggleButtonClick="Snde_ToggleButtonClick" FuncTextMargin="">
                                <controls:FunctControl.Style>
                                    <Style TargetType="controls:FunctControl" BasedOn="{StaticResource FuncGlobalStyle2}">
                                        <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=snde/LeftLabel/@value}" />
                                        <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=snde/RightLabel/@value}" />
                                        <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=snde/FuncText/@value}" />
                                        <Setter Property="FuncButtonIsChecked" Value="False" />
                                        <Style.Triggers>
                                            <Trigger Property="FuncButtonIsChecked" Value="True">
                                                <Setter Property="FuncBackground" Value="{Binding Source={StaticResource configurationUI}, XPath=BackgroundFunc/@value}" />
                                                <Setter Property="FuncForeground" Value="{Binding Source={StaticResource configurationUI}, XPath=LeftLabelForeground/@value}" />
                                                <Setter Property="FuncTextSize" Value="12"/>
                                            </Trigger>
                                            <DataTrigger Binding="{Binding ElementName=Alpha, Path=FuncButtonIsChecked}" Value="True">
                                                <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=snde/RightLabel/@value}" />
                                                <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=snde/LeftLabel/@value}" />
                                                <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=snde/FuncText/@value}" />
                                                <Setter Property="FuncLabelLeftMargin" Value="-5 0 0 0" />
                                                <Setter Property="FuncLabelRightMargin" Value="-20 0 0 0" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </controls:FunctControl.Style>
                            </controls:FunctControl>
                            <controls:FunctControl x:Name="Quitter" FuncTextMargin="" ToggleButtonClick="Quitter_ToggleButtonClick">
                                <controls:FunctControl.Style>
                                    <Style TargetType="controls:FunctControl" BasedOn="{StaticResource FuncGlobalStyle2}">
                                        <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=quitter/LeftLabel/@value}" />
                                        <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=quitter/RightLabel/@value}" />
                                        <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=quitter/FuncText/@value}" />

                                        <Setter Property="FuncLabelLeftMargin" Value="0" />
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding ElementName=Snde, Path=FuncButtonIsChecked}" Value="True">
                                                <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=quitter/LeftLabel/@value}" />
                                                <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=quitter/FuncText/@value}" />
                                                <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=quitter/RightLabel/@value}" />
                                                <Setter Property="FuncTextSize" Value="11" />
                                                <Setter Property="FuncLabelLeftMargin" Value="-5" />
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding ElementName=Alpha, Path=FuncButtonIsChecked}" Value="True">
                                                <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=quitter/RightLabel/@value}" />
                                                <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=quitter/LeftLabel/@value}" />
                                                <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=quitter/FuncText/@value}" />
                                                <Setter Property="FuncLabelLeftMargin" Value="-10 0 0 0" />
                                                <Setter Property="FuncLabelRightMargin" Value="-12 0 0 0" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </controls:FunctControl.Style>
                            </controls:FunctControl>
                            <controls:FunctControl x:Name="Inserer" FuncTextMargin="" ToggleButtonClick="Inserer_ToggleButtonClick">
                                <controls:FunctControl.Style>
                                    <Style TargetType="controls:FunctControl" BasedOn="{StaticResource FuncGlobalStyle2}">
                                        <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=inserer/LeftLabel/@value}" />
                                        <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=inserer/RightLabel/@value}" />
                                        <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=inserer/FuncText/@value}" />
                                        <Setter Property="FuncLabelLeftMargin" Value="0" />
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding ElementName=Snde, Path=FuncButtonIsChecked}" Value="True">
                                                <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=inserer/LeftLabel/@value}" />
                                                <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=inserer/FuncText/@value}" />
                                                <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=inserer/RightLabel/@value}" />
                                                <Setter Property="FuncTextSize" Value="11" />
                                                <Setter Property="FuncLabelLeftMargin" Value="-5" />
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding ElementName=Alpha, Path=FuncButtonIsChecked}" Value="True">
                                                <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=inserer/RightLabel/@value}" />
                                                <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=inserer/LeftLabel/@value}" />
                                                <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=inserer/FuncText/@value}" />
                                                <Setter Property="FuncLabelLeftMargin" Value="-8 0 0 0" />
                                                <Setter Property="FuncLabelRightMargin" Value="-12 0 0 0" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </controls:FunctControl.Style>
                            </controls:FunctControl>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <controls:FunctControl x:Name="Alpha" ToggleButtonClick="Alpha_ToggleButtonClick" FuncTextMargin="">
                                <controls:FunctControl.Style>
                                    <Style TargetType="controls:FunctControl" BasedOn="{StaticResource FuncGlobalStyle2}">
                                        <Setter Property="FuncButtonIsChecked" Value="False" />
                                        <Setter Property="FuncBackground" Value="{StaticResource RightLabelForeground}" />
                                        <Setter Property="FuncForeground" Value="{StaticResource FuncControl1Background}" />
                                        <Setter Property="LeftLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=alpha/LeftLabel/@value}" />
                                        <Setter Property="RightLabelText" Value="{Binding Source={StaticResource configurationUI}, XPath=alpha/RightLabel/@value}" />
                                        <Setter Property="FuncText" Value="{Binding Source={StaticResource configurationUI}, XPath=alpha/FuncText/@value}" />
                                        <Style.Triggers>
                                            <Trigger Property="FuncButtonIsChecked" Value="True">
                                                <Setter Property="FuncBackground" Value="{Binding Source={StaticResource configurationUI}, XPath=BackgroundFunc/@value}" />
                                                <Setter Property="FuncForeground" Value="{Binding Source={StaticResource configurationUI}, XPath=RightLabelForeground/@value}" />
                                                <Setter Property="FuncTextSize" Value="12"/>
                                            </Trigger>
                                            <DataTrigger Binding="{Binding ElementName=Snde, Path=FuncButtonIsChecked}" Value="True">
